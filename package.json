{
  "name": "c4-rms-excel-add-in",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://capital-four.com/"
  },
  "license": "",
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000,
    "source-bundle-url-path": "index.win32"
  },
  "scripts": {
    "build-prod": "webpack -p --mode production --env.deploymentUrlPaths=production",
    "build-uat": "webpack -p --mode production --env.deploymentUrlPaths=uat",
    "build-dev": "webpack --mode development  --env.deploymentUrlPaths=development",
    "dev-server": "webpack-dev-server --mode development",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "sideload:excel": "office-toolbox sideload -m manifest.xml -a excel",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "unload": "office-toolbox remove -m manifest.xml -a excel",
    "unload:excel": "office-toolbox remove -m manifest.xml -a excel",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch",
    "deploy": "az webapp up --location CentralUS --name cfaddin-development --html"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@types/custom-functions-runtime": "^1.4.11",
    "@types/office-js": "^1.0.1",
    "@types/office-runtime": "^1.0.8",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "4.4.22",
    "@types/webpack-dev-server": "3.1.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.5.3",
    "custom-functions-metadata-plugin": "^0.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^6.1.0",
    "msal": "^1.1.3",
    "office-addin-debugging": "^2.1.13",
    "office-addin-dev-certs": "^1.0.1",
    "office-toolbox": "^0.1.1",
    "office-ui-fabric-react": "^6.138.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^6.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.0",
    "ts-loader": "^5.4.4",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
